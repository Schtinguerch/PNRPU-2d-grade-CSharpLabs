<Window x:Class="WpfFileWorking.Views.NewCarVindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:sys="clr-namespace:System;assembly=mscorlib"
        xmlns:local="clr-namespace:WpfFileWorking.Views"
        mc:Ignorable="d"
        Title="NewCarVindow" Height="100" Width="400">
    <Grid>
        <StackPanel Orientation="Vertical">
            <ComboBox 
                Name="CarComboBox"
                Margin="6"
                SelectionChanged="Selector_OnSelectionChanged">
                
                <sys:String>Вагон эконом-класса</sys:String>
                <sys:String>Вагон-купе</sys:String>
                <sys:String>Вагон-кухня</sys:String>
                <sys:String>Локомотив</sys:String>
            </ComboBox>
        </StackPanel>
    </Grid>
</Window>
